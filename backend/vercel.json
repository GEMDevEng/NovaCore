{
  "version": 2,
  "buildCommand": "npm ci",
  "outputDirectory": null,
  "env": {
    "NODE_ENV": "production"
  },
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 30,
        "memory": 1024
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ]
}

